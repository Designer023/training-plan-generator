{
  "name": "@designer023/training-plan-generator",
  "version": "0.1.0",
  "description": "A running training plan generator",
  "main": "index.js",
  "author": "Carl Topham",
  "license": "MIT",
  "private": true,
  "dependencies": {
    "bootstrap": "^4.3.1",
    "connected-react-router": "6.5.2",
    "history": "^4.10.1",
    "moment": "^2.24.0",
    "node-sass": "^4.14.1",
    "react": "^16.11.0",
    "react-dates": "21.8.0",
    "react-dom": "^16.11.0",
    "react-hot-loader": "^4.12.21",
    "react-redux": "^7.2.0",
    "react-router": "5.0.1",
    "react-router-dom": "5.0.1",
    "react-scripts": "3.2.0",
    "redux": "^4.0.4",
    "redux-actions": "2.6.5",
    "redux-form": "^8.2.6",
    "redux-thunk": "^2.3.0",
    "typescript": "3.4.3"
  },
  "devDependencies": {
    "@types/jest": "22.2.3",
    "@types/lodash": "4.14.149",
    "@types/node": "12.12.2",
    "@types/react": "16.8.24",
    "@types/react-dom": "16.0.5",
    "@types/react-redux": "7.1.6",
    "husky": "^2.1.0",
    "prettier": "1.17.0",
    "pretty-quick": "^1.10.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "coverage": "set CI=true && react-scripts test --coverage --collect-coverage",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged --pattern '**/*.*(js|jsx)'"
    }
  },
  "jest": {
    "coverageReporters": [
      "json",
      "lcov",
      "text",
      "html"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!**/node_modules/**",
      "!**/coverage/**",
      "!**/serviceWorker.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    }
  }
}
